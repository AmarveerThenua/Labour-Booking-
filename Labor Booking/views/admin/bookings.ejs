<%- include('../partials/header') %>

<h2 class="mb-4">All Bookings</h2>

<table class="table table-bordered">
  <thead>
    <tr>
      <th>User</th>
      <th>Labor</th>
      <th>Area</th>
      <th>Booking Date</th>
      <th>Status</th>
    </tr>
  </thead>
  <tbody>
    <% bookings.forEach(b => { %>
      <tr>
        <td><%= b.user.name %> (<%= b.user.email %>)</td>
        <td><%= b.labor.user.name %></td>
        <td><%= b.labor.area %></td>
        <td><%= b.date.toLocaleDateString() %></td>
        <td><%= b.status %></td>
      </tr>
    <% }) %>
  </tbody>
</table>

<%- include('../partials/footer') %>
